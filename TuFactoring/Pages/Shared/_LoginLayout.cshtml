@using System.Globalization;
@{
    var culture = System.Globalization.CultureInfo.CurrentCulture.Name;
}
<!DOCTYPE html>
<html lang="@CultureInfo.CurrentCulture.ThreeLetterWindowsLanguageName">
<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">

    <title>@ViewData["Title"] - @Localizer.Text("branding")</title>

    <!-- Custom fonts for this template-->
    <link href="~/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i" rel="stylesheet">


    <link href="~/css/sb-admin-2.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/@@mdi/font@3.x/css/materialdesignicons.min.css" rel="stylesheet">



    <!-- Custom styles for this template-->
    <link href="https://cdn.jsdelivr.net/npm/@@mdi/font@3.x/css/materialdesignicons.min.css" rel="stylesheet">

    <link rel="stylesheet" href="~/css/toastr.css">

</head>

<body class="bg-gradient-@ViewData["bgcolor"]">
    <div class="container">
        @RenderBody()
    </div>

    <!-- Bootstrap core JavaScript-->
    <script src="~/vendor/jquery/jquery.min.js"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

    <!-- Core plugin JavaScript-->
    <script src="~/vendor/jquery-easing/jquery.easing.min.js"></script>

    <script src="~/js/Vue/vue.js"></script>
    <script src="~/js/Vue/vuei18n.min.js" asp-append-version="true"></script>

    <script src="~/js/translate/idiomas/es.js" asp-append-version="true"></script>
    <script src="~/js/translate/idiomas/es-VE.js" asp-append-version="true"></script>
    <script src="~/js/translate/idiomas/es-DO.js" asp-append-version="true"></script>
    <script src="~/js/translate/idiomas/en.js" asp-append-version="true"></script>
    <script src="~/js/translate/diccionario.js" asp-append-version="true"></script>

    <!-- Custom scripts for all pages-->
    <script src="~/js/sb-admin-2.min.js"></script>
    <script src="~/js/toastr.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>



    <script src="~/js/msgPoppup.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>
