@page
@namespace TuFactoring.Areas.Profile.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@inject Utilities.CultureLocalizer Localizer

@model TuFactoring.Areas.Profile.Pages.ActualizarEmpresaModel
@using System.Globalization;
@{
    ViewData["Title"] = "Perfil";

    switch (@Model.TipoParticipante)
    {
        case "CONFIRMANT":
            ViewData["Sidebar"] = "dark";
            break;
        case "DEBTOR":
            ViewData["Sidebar"] = "primary";
            break;
        case "FACTOR":
            ViewData["Sidebar"] = "warning";
            break;
        case "SUPPLIER":
            ViewData["Sidebar"] = "purple";
            break;
        default:
            ViewData["Sidebar"] = "navy";
            break;
    }

    Layout = "~/Pages/Shared/_Layout.cshtml";
    var culture = System.Globalization.CultureInfo.CurrentCulture.Name;
}

<v-app id="appRegistroEmpresas">
    <div class="mt-5">
        @Html.AntiForgeryToken()
        <input hidden id="contenidoRaw" value="@Model.registroJson" />

        <div class="modal fade in fa fa-spinner" v-if="cargando" role="dialog">
            <div class="modal-dialog text-center">

                <h2 style="color:#000"><span id="cargando">Cargando...</span></h2>
            </div>
        </div>

        <div id="contenido" hidden>
            <div class="d-sm-flex align-items-center justify-content-between mb-4">
                <h1 class="h3 mb-0 text-gray-800">
                    <a href="#" style="color:#fff !important" class="btn btn-success btn-circle">
                        <i class="far fa-id-badge"></i>
                    </a>
                    &nbsp;@Localizer.Text("titlePerfil")
                </h1>
                <span style="display: none;">
                    <a href="#" class="d-none d-sm-inline-block btn btn-sm btn-primary shadow-sm"><i class="fas fa-download fa-sm text-white-50"></i> Generate Report</a>
                </span>
            </div>
            <p class="mb-4">
                <span v-if="registro.user.participant == 'DEBTOR'">@Localizer.Text("textoAyudaPerfil")</span>
                <span v-if="registro.user.participant == 'FACTOR' && registro.user.discriminator=='LEGAL'">@Localizer.Text("textoAyudaPerfilInversionista")</span>
                <span v-if="registro.user.participant == 'FACTOR' && registro.user.discriminator=='PERSON'">@Localizer.Text("textoAyudaPerfilInversionistaPerson")</span>
                <span v-if="registro.user.participant == 'SUPPLIER'">@Localizer.Text("textoAyudaPerfilProveedor")</span>
                <span v-if="registro.user.participant == 'CONFIRMANT'">@Localizer.Text("textoAyudaPerfilBanco")</span>
                <span v-if="registro.user.participant == 'BACKOFFICE'">@Localizer.Text("textoAyudaPerfilOperativo")</span>

                <br />
                <br />@Localizer.Text("textoPress")<a v-on:click="dialogAyuda=true" href="#" class="font-weight-bold"> @Localizer.Text("textoHere")</a> @Localizer.Text("textoMoreInformation")
            </p>

            <v-dialog v-model="dialogAyuda" max-width="500" transition="dialog-bottom-transition">
                <v-card>
                    <v-card-title>
                        <v-spacer></v-spacer>
                        <v-btn icon @@click="dialogAyuda = false">
                            <v-icon>@Localizer.Text("iconCloseMDI")</v-icon>
                        </v-btn>
                    </v-card-title>
                    <v-card-text>
                        <v-container>
                            <v-row>
                                <v-col cols="12" sm="12" lg="12" md="12">

                                    <div v-if="registro.user.participant == 'DEBTOR'">
                                        <p><strong>@Localizer.Text("textoModalAyudaPerfilEmpresatitulo1")</strong></p>
                                        <p>@Localizer.Text("textoModalAyudaPerfilEmpresatexto1")</p>

                                        <p><strong>@Localizer.Text("textoModalAyudaPerfilEmpresatitulo2")</strong></p>
                                        <p>
                                            @Localizer.Text("textoModalAyudaPerfilEmpresatexto2")<br />
                                            @Localizer.Text("textoModalAyudaPerfilEmpresatexto4")
                                        </p>

                                        <p><strong>@Localizer.Text("textoModalAyudaPerfilEmpresatitulo3")</strong></p>
                                        <p>@Localizer.Text("textoModalAyudaPerfilEmpresatexto3")</p>
                                    </div>

                                    <div v-if="registro.user.participant == 'SUPPLIER'">
                                        <p><strong>@Localizer.Text("textoModalAyudaPerfilEmpresatitulo1")</strong></p>
                                        <p>@Localizer.Text("textoModalAyudaPerfilEmpresatexto1")</p>

                                        <p><strong>@Localizer.Text("textoModalAyudaPerfilEmpresatitulo2")</strong></p>
                                        <p>
                                            @Localizer.Text("textoModalAyudaPerfilEmpresatexto2")<br />
                                            @Localizer.Text("textoModalAyudaPerfilEmpresatexto4")
                                        </p>

                                        <p><strong>@Localizer.Text("textoModalAyudaPerfilEmpresatitulo3")</strong></p>
                                        <p>@Localizer.Text("textoModalAyudaPerfilProveedortexto4")</p>
                                    </div>

                                    <div v-if="registro.user.participant == 'FACTOR' && registro.user.discriminator=='LEGAL'">
                                        <p>@Localizer.Text("textoModalAyudaPerfilInversionistaLegalTexto1")</p>
                                        <p>
                                            @Localizer.Text("textoModalAyudaPerfilInversionistaLegalTexto2")<br />
                                            @Localizer.Text("textoModalAyudaPerfilInversionistaLegalTexto3")
                                        </p>
                                        <p>@Localizer.Text("textoModalAyudaPerfilInversionistaLegalTexto4")</p>
                                    </div>

                                    <div v-if="registro.user.participant == 'FACTOR' && registro.user.discriminator=='PERSON'">
                                        <p>@Localizer.Text("textoModalAyudaPerfilInversionistaPersonTexto1")</p>
                                        <p>@Localizer.Text("textoModalAyudaPerfilInversionistaLegalTexto4")</p>
                                    </div>

                                    <div v-if="registro.user.participant == 'CONFIRMANT'">
                                        <p>@Localizer.Text("textoModalAyudaPerfilBancoTexto1")</p>
                                        <p>
                                            @Localizer.Text("textoModalAyudaPerfilInversionistaLegalTexto2")<br />
                                            @Localizer.Text("textoModalAyudaPerfilInversionistaLegalTexto3")
                                        </p>
                                        <p>@Localizer.Text("textoModalAyudaPerfilInversionistaLegalTexto4")</p>
                                    </div>

                                    <div v-if="registro.user.participant == 'BACKOFFICE'">
                                        <p><strong>@Localizer.Text("textoModalAyudaPerfilEmpresatitulo1")</strong></p>
                                        <p>@Localizer.Text("textoModalAyudaPerfilEmpresatexto1")</p>

                                        <p><strong>@Localizer.Text("textoModalAyudaPerfilEmpresatitulo2")</strong></p>
                                        <p>
                                            @Localizer.Text("textoModalAyudaPerfilEmpresatexto2")<br />
                                            @Localizer.Text("textoModalAyudaPerfilEmpresatexto4")
                                        </p>
                                    </div>

                                </v-col>
                            </v-row>
                        </v-container>
                    </v-card-text>
                </v-card>
            </v-dialog>

            <div class="row">
                <div class="col-sm-12">
                    <div class="">

                        <v-expand-transition>

                            <div class="row" v-show="botonSiguiente == 0">
                                <!---------------------------------------------- caja Datos Comerciales ---------------------------------------------------------->
                                <div class="col-lg-6">
                                    <div class="card shadow mb-4">
                                        <div class="card-header py-3">
                                            <h6 class="m-0 font-weight-bold text-primary">
                                                <i class="@Localizer.Text("iconCommercialData") logoColor"></i> &nbsp;
                                                <span v-if="registro.user.discriminator == 'PERSON'">@Localizer.Text("titleDatosPerson")</span>
                                                <span v-else>@Localizer.Text("titleCommercialData")</span>
                                            </h6>
                                        </div>
                                        <div class="card-body">
                                            <div class="row">
                                                <div class="col-sm-12" v-for="iden in dataPaises.identifications" v-if="iden.default && iden.discriminator == registro.user.discriminator">
                                                    <div class="from-group">
                                                        <label>{{iden.name}}</label>
                                                        <div class="input-group">
                                                            <div v-if="iden.prefix" class="input-group-prepend">

                                                                <select disabled class="input-group-text" v-on:change="validarDocumentoPrincipal(iden.regexp, iden.mask_edit)" v-model="registro.registrarse.document.prefix">
                                                                    <option v-for="pref in iden.prefixes" :value="pref.id">{{pref.abbreviation}}</option>
                                                                </select>
                                                            </div>
                                                            <div v-else class="input-group-prepend">
                                                                <select disabled v-on:change="validarDocumentoPrincipal(iden.regexp, iden.mask_edit)" class="input-group-text">
                                                                    <option selected :value="registro.registrarse.document.prefix = null">{{iden.abbreviation}}</option>
                                                                </select>
                                                            </div>
                                                            <input disabled onPaste="return false" :maxlength="iden.digits" v-on:blur="validarDocumentoPrincipal(iden.regexp, iden.mask_edit)" v-model="registro.registrarse.document.number" :class="['form-control',errorDocumento]" :placeholder="iden.mask_edit">
                                                        </div>
                                                        <span class="text-danger">{{errorTextoDocumentoPrincipal}}</span>
                                                    </div>

                                                </div>
                                                <div class="from-group col-sm-12">
                                                    <label v-if="registro.user.discriminator=='LEGAL'">@Localizer.Text("textocompanyName")</label>
                                                    <label v-else>@Localizer.Text("textonameSurname")</label>

                                                    <input v-if="registro.user.discriminator=='LEGAL'" maxlength="255" disabled v-on:keyup="validarNombreLegal()" v-model="registro.registrarse.company" :class="boleanoDesicionRegistro == false ? errorNombreLegal = 'is-valid' : errorNombreLegal" class="form-control" placeholder="@Localizer.Text("placeholderNombreLegal")">
                                                    <input v-else maxlength="255" disabled v-on:blur="validarNombresPerson()" v-model="registro.registrarse.firstName" :class="boleanoDesicionRegistro == false ? errorNombreLegal = 'is-valid' : errorNombreLegal" class="form-control" placeholder="@Localizer.Text("textonameSurname")">
                                                    <span class="text-danger">{{boleanoDesicionRegistro == false ? errorTextoNombreLegal = '' : errorTextoNombreLegal }}</span>
                                                </div>

                                                <div v-if="registro.user.discriminator=='LEGAL'" class="from-group col-sm-12">
                                                    <label for="proposito">@Localizer.Text("textocomercialActivity")</label>

                                                    <select disabled v-on:blur="validarPurpuse()" v-on:change="validarPurpuse()" v-model="registro.registrarse.category" :class="['form-control',errorPurpose]">
                                                        <option v-if="registro.registrarse.category == '' || registro.registrarse.category ==null" :value="registro.registrarse.category" disabled="disabled">@Localizer.Text("placeholderActivity")</option>
                                                        <option v-for="purp in dataPaises.categories" :value="purp.id">{{purp.name}}</option>
                                                    </select>
                                                    <span class="text-danger">{{ errorTextoPurpose }}</span>
                                                </div>

                                                <div v-else class="from-group col-sm-12">
                                                    <label for="proposito">@Localizer.Text("textooccupation")</label>
                                                    <select disabled v-on:change="validarOcupacion()" v-on:blur="validarOcupacion()" v-model="registro.registrarse.category" :class="['form-control',errorOccupation]">
                                                        <option disabled v-if="registro.registrarse.category == '' || registro.registrarse.category ==null" :value="registro.registrarse.category">@Localizer.Text("placeholderOcupation")</option>
                                                        <option v-for="ocup in dataPaises.categories" :value="ocup.id">{{ocup.name}}</option>
                                                    </select>
                                                    <span class="text-danger">{{ errorOccupationTexto }}</span>
                                                </div>

                                                <div v-if="registro.user.discriminator != 'LEGAL' && registro.user.participant == 'FACTOR'" class="from-group col-sm-12">
                                                    <label for="email">@Localizer.Text("textoEmail")</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text"><i class="fa fa-envelope"></i></span>
                                                        </div>
                                                        <input disabled onPaste="return false" maxlength="60" v-on:keyup="validarEmailNatural()" v-model="registro.registrarse.email.address" :class="['form-control',errorEmailNatural]" placeholder="@Localizer.Text("placeholderEmail")">
                                                    </div>
                                                    <span class="text-danger">{{errorEmailNaturalTexto}}</span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!----------------------------------------------Panel Direcciones ---------------------------------------------------------->
                                <div class="col-lg-6">
                                    <div class="card shadow mb-4">
                                        <div class="card-header py-3">
                                            <h6 class="m-0 font-weight-bold text-primary"><i class="@Localizer.Text("iconTitleAddress") logoColor"></i> &nbsp;@Localizer.Text("titleAddress")</h6>
                                        </div>
                                        <div class="card-body">
                                            <div class="row">
                                                <div class="from-group col-sm-12">
                                                    <label for="">@Localizer.Text("textofiscalAddress")</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text"><i class="@Localizer.Text("iconTitleAddress")"></i></span>
                                                        </div>
                                                        <input :disabled="registro.authRol == false" maxlength="255" v-on:keyup="validarDireccion" v-model="registro.registrarse.address.line1" :class="['form-control',errorDireccion]" placeholder="@Localizer.Text("placeholderEnterYourAddress")">
                                                    </div>
                                                    <span class="text-danger">{{errorTextoDireccion}}</span>
                                                    <div role="separator" class="divider"></div>
                                                </div>
                                                <div class="from-group col-sm-12">
                                                    <div class="input-group">
                                                        <input :disabled="registro.authRol == false" maxlength="255" :class="['form-control',errorDireccion2]" v-on:keyup="validarDireccion2" v-model="registro.registrarse.address.line2" placeholder="@Localizer.Text("placeholderEnterYourAddress2")">
                                                    </div>
                                                    <span class="text-danger">{{errorDireccion2Texto}}</span>
                                                    <div role="separator" class="divider"></div>
                                                </div>
                                                <div class="from-group col-sm-12">
                                                    <label>@Localizer.Text("textostate")</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text"><i class="@Localizer.Text("iconAddress")"></i></span>
                                                        </div>
                                                        <select :disabled="registro.authRol == false" v-on:blur="restablecerCiudad()" v-on:change="restablecerCiudad()" v-model="idCiudad" :class="['form-control']">
                                                            <option disabled v-if="idCiudad== 0 || idCiudad==null " value="0">@Localizer.Text("placeholderEstate")</option>
                                                            <option v-for="(estados,index) in dataPaises.regions" :value="estados.id">{{estados.name}}</option>
                                                        </select>

                                                    </div>
                                                    <span class="text-danger">{{errorEstadoTexto}}</span>
                                                </div>
                                                <div v-if="cambiarCiudad != 0" :class="['from-group col-sm-12',errorCiudad]">
                                                    <div>
                                                        <label>@Localizer.Text("textocity")</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text"><i class="@Localizer.Text("iconCity")"></i></span>
                                                            </div>
                                                            <select :disabled="registro.authRol == false" :class="['form-control',errorCiudad]" v-on:blur="validarCiudad()" v-on:change="validarCiudad()" v-model="registro.registrarse.address.city">
                                                                <option disabled v-if="registro.registrarse.address.city == '' || registro.registrarse.address.city == null" :value="registro.registrarse.address.city">@Localizer.Text("placeholderCity")</option>
                                                                <option v-for="ciudadActual of listCities" :value="ciudadActual.id">{{ciudadActual.name}}</option>
                                                            </select>
                                                        </div>
                                                        <span class="text-danger">{{errorCiudadTexto}}</span>
                                                    </div>
                                                </div>
                                                <div class="from-group col-sm-12">
                                                    <label for="telefonoL">@Localizer.Text("textophone")</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text"><i class="@Localizer.Text("iconPhone")"></i></span>
                                                        </div>
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text">+{{dataPaises.calling_code}}</span>
                                                        </div>
                                                        <input :disabled="registro.authRol == false" :maxlength="maxlegthTelefono" onPaste="return false" :class="['form-control', errorTelefonoDirecciones]" v-on:keyup="validarTelefonoDirecciones()" type="text" v-model="registro.registrarse.phone.number" :placeholder="placeholderTelefono">
                                                    </div>
                                                    <span class="text-danger">{{errorTextoTelefonoDirecciones}}</span>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                            </div>

                        </v-expand-transition>

                        <v-expand-transition>
                            <div class="row" v-show="botonSiguiente == 1">

                                <!---------------------------------------------- caja Representante Legal ---------------------------------------------------------->
                                <div v-if="registro.user.discriminator == 'LEGAL'" class="col-lg-6">
                                    <div>
                                        <div v-if="registro.representante != null">
                                            <div class="card shadow mb-4">
                                                <div class="card-header py-3">
                                                    <h6 class="m-0 font-weight-bold text-primary"><i class="@Localizer.Text("iconLegalRepre") logoColor"></i> &nbsp;@Localizer.Text("titleLegalRepresentative")</h6>
                                                </div>
                                                <div class="card-body">
                                                    <div class="row">
                                                        <div class="col-sm-12" v-for="iden in dataPaises.identifications" v-if="iden.default && iden.discriminator == 'PERSON'">

                                                            <div class="from-group">
                                                                <span v-if="iden.id !== 999 ? registro.representante.identification = iden.id : iden.id = 9999 "></span>

                                                                <label for="doc">{{iden.name}}</label>
                                                                <div class="input-group">
                                                                    <div v-if="iden.prefix" class="input-group-prepend">
                                                                        <select disabled v-on:change="validarDocumentoRepresentanteLegal(iden.regexp, iden.mask_edit)" v-model="registro.representante.prefix" class="input-group-text">
                                                                            <option v-for="pref in iden.prefixes" v-if="pref.status == true" :value="pref.id">{{pref.abbreviation}}</option>
                                                                        </select>
                                                                    </div>
                                                                    <div v-else class="input-group-prepend">
                                                                        <select disabled v-on:change="validarDocumentoRepresentanteLegal(iden.regexp, iden.mask_edit)" class="input-group-text">
                                                                            <option selected :value="registro.representante.prefix = null">{{iden.abbreviation}}</option>
                                                                        </select>
                                                                    </div>
                                                                    <input disabled onPaste="return false" :maxlength="iden.digits" v-on:keyup="validarDocumentoRepresentanteLegal(iden.regexp, iden.mask_edit)" v-model="registro.representante.documentNumber" :class="['form-control']" :placeholder="iden.mask_edit">
                                                                </div>
                                                                <span class="text-danger">{{errorTextoDocumentoRepresentanteLegal}}</span>
                                                            </div>

                                                        </div>
                                                        <div class="from-group col-sm-12">
                                                            <label for="nombre">@Localizer.Text("textonameSurname")</label>
                                                            <div class="input-group">
                                                                <div class="input-group-prepend">
                                                                    <span class="input-group-text"><i class="@Localizer.Text("iconUser")"></i></span>
                                                                </div>
                                                                <input disabled maxlength="255" v-on:keyup="validarNombresRepresentanteLegal()" v-model="registro.representante.name" :class="['form-control']" placeholder="@Localizer.Text("placeholderName")">
                                                            </div>
                                                            <span class="text-danger">{{errorTextoNombresRepresentanteLegal}} {{errorTextoApellidosRepresentanteLegal}}</span>
                                                        </div>

                                                    </div>
                                                    <div class="row">
                                                        <div class="from-group col-sm-12">
                                                            <label for="telefonoL">@Localizer.Text("textophone")</label>
                                                            <div class="input-group">
                                                                <div class="input-group-prepend">
                                                                    <span class="input-group-text"><i class="@Localizer.Text("iconPhone")"></i></span>
                                                                </div>
                                                                <div class="input-group-prepend">
                                                                    <span class="input-group-text">+{{dataPaises.calling_code}}</span>
                                                                </div>
                                                                <input :disabled="registro.authRol == false" onPaste="return false" :maxlength="maxlegthTelefono" v-on:keyup="validarTelefonoRepresentanteLegal()" type="text" v-model="registro.representante.phoneNumber" :class="['form-control',errorTelefonoRepresentante]" :placeholder="placeholderTelefono">

                                                            </div>
                                                            <span class="text-danger">{{errorTextoTelefonoRepresentante}}</span>
                                                        </div>
                                                        <div class="from-group col-sm-12">
                                                            <label for="email">@Localizer.Text("textoEmail")</label>
                                                            <div class="input-group">
                                                                <div class="input-group-prepend">
                                                                    <span class="input-group-text"><i class="@Localizer.Text("iconEmail")"></i></span>
                                                                </div>
                                                                <input disabled onPaste="return false" maxlength="60" v-on:keyup="validarEmailRepresentanteLegal()" v-model="registro.representante.email" :class="['form-control']" placeholder="@Localizer.Text("placeholderEmail")">

                                                            </div>
                                                            <span class="text-danger">{{errorTextoEmailRepresentante}}</span>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <!----------------------------------------------- caja Administrado ----------------------------------------------------- -->
                                <div v-if="registro.user.participant == 'CONFIRMANT'" class="col-lg-6">
                                    <div class="card shadow mb-4">
                                        <div class="card-header py-3">
                                            <h6 class="m-0 font-weight-bold text-primary"><i class="@Localizer.Text("iconAdmin") logoColor"></i> &nbsp;@Localizer.Text("titleAdministrator")</h6>
                                        </div>
                                        <div class="card-body">
                                            <div class="row">
                                                <div class="col-sm-12" v-for="iden in dataPaises.identifications" v-if="iden.default && iden.discriminator == 'PERSON'">
                                                    <span v-if="iden.id !== 999 ? registro.administrador.identification = iden.id : iden.id = 9999 "></span>

                                                    <label for="doc">{{iden.name}}</label>
                                                    <div class="input-group">
                                                        <div v-if="iden.prefix" class="input-group-prepend">
                                                            <select :disabled="registro.authRol == false" v-on:change="validarDocumentoAdministrador(iden.regexp, iden.mask_edit)" v-model="registro.administrador.prefix" class="input-group-text">
                                                                <option v-for="pref in iden.prefixes" v-if="pref.status == true" :value="pref.id">{{pref.abbreviation}}</option>
                                                            </select>
                                                        </div>
                                                        <div class="input-group-prepend" v-else>
                                                            <select disabled v-on:change="validarDocumentoAdministrador(iden.regexp, iden.mask_edit)" class="input-group-text">
                                                                <option selected :value="registro.administrador.prefix = null">{{iden.abbreviation}}</option>
                                                            </select>
                                                        </div>
                                                        <input :disabled="registro.authRol == false" onPaste="return false" :maxlength="iden.digits" v-on:keyup="validarDocumentoAdministrador(iden.regexp, iden.mask_edit)" v-model="registro.administrador.documentNumber" :class="['form-control', errorDocumentoAdministrador]" :placeholder="iden.mask_edit">
                                                    </div>
                                                    <span class="text-danger">{{errorDocumentoAdministradorTexto}}</span>
                                                </div>
                                                <div class="from-group col-sm-12">
                                                    <label for="txtNombreContactoCliente">@Localizer.Text("textonameSurname")</label>
                                                    <div class="input-group">

                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text"><i class="@Localizer.Text("iconUser")"></i></span>
                                                        </div>
