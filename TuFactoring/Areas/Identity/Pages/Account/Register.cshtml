@page
@using System.Globalization;

@model RegisterModel
@{
    //TuFactoring.Areas.Registry.Pages.RegistroTuFactoringModel
    ViewData["_LayoutRegistro"] = "appRegistroEmpresas";
    Layout = ViewData["Layout"].ToString();

    switch (@Model.Rol)
    {
        case 1:
            ViewData["Title"] = Localizer.Text("titleDebtorRegister");
            break;
        case 2:
            ViewData["Title"] = Localizer.Text("titleProviderRegister");
            break;
        case 3:
            ViewData["Title"] = Localizer.Text("titleFactorRegister");
            break;
        case 4:
            ViewData["Title"] = Localizer.Text("titleFactorRegister");
            break;

    }

    switch (@Model.TipoParticipante)
    {
        case "CONFIRMANT":
            ViewData["Sidebar"] = "dark";
            ViewData["Title"] = Localizer.Text("titleDebtorRegister");
            break;
        case "DEBTOR":
            ViewData["Sidebar"] = "primary";
            break;
        case "FACTOR":
            ViewData["Sidebar"] = "warning";
            break;
        case "SUPPLIER":
            ViewData["Sidebar"] = "purple";
            break;
        case "BACKOFFICE":
            ViewData["Sidebar"] = "navy";
            ViewData["Title"] = Localizer.Text("titleBankRegister");
            break;
        default:
            ViewData["Sidebar"] = "navy";
            break;
    }

    var culture = System.Globalization.CultureInfo.CurrentCulture.Name;
}

